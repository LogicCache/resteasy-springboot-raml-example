{
  "$schema": "http://json-schema.org/schema",
  "type": "object",
  "description": "An Album",
  "id": "Album",
  "javaType": "com.logiccache.domain.Album",
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "integer",
      "required": false
    },
    "title": {
      "type": "string",
      "required": true
    },
    "artist": {
      "type": "string",
      "required": true
    },
    "released": {
      "type": "string",
      "required": true
    }
  }
}
